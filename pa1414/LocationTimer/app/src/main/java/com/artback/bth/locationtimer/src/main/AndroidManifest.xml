<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.stephenbaidu.placepicker">

    <application android:allowBackup="true" >
        <activity
            android:name=".PlacePicker"
            android:label="@string/app_name" >
            <meta-data
                android:name="android.app.place_picker_searchable"
                android:resource="@xml/place_picker_searchable" />
        </activity>
        <activity
            android:name=".RecentSearchesActivity"
            android:label="@string/title_activity_recent_searches"
            android:parentActivityName=".PlacePicker" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PlacePicker" />
        </activity>
    </application>

</manifest>
